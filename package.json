    "@typescript-eslint/parser": "^4.21.0",
    "babel-jest": "^26.6.3",
    "bufferutil": "^4.0.3",
    "canvas": "^2.7.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.4",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "utf-8-validate": "^5.0.4"
